{
  "name": "spectacle-boilerplate",
  "version": "1.0.1",
  "description": "ReactJS Powered Presentation Framework",
  "main": "lib/index.js",
  "repository": "https://github.com/aleccool213/mutation-testing-talk",
  "scripts": {
    "clean": "rimraf dist",
    "build":
      "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
